{
	"name": "@polymedia/profile-web",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "vite build",
		"clean": "rm -rf dist/ node_modules/ .turbo/ .wrangler/",
		"deploy-testnet": "pnpm build && wrangler pages deploy dist/ --project-name polymedia-profile --branch testnet",
		"deploy-prod": "pnpm build && wrangler pages deploy dist/ --project-name polymedia-profile --branch main",
		"dev": "vite",
		"host": "vite --host",
		"lint:fix": "biome check --write .",
		"lint:unsafe": "biome check --write --unsafe .",
		"lint": "biome check .",
		"preview": "pnpm build && vite preview",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@mysten/dapp-kit": "^0.17.1",
		"@mysten/sui": "^1.37.0",
		"@mysten/walrus": "^0.5.2",
		"@polymedia/profile-sdk": "workspace:*",
		"@polymedia/suitcase-core": "^0.0.62",
		"@polymedia/suitcase-react": "^0.0.62",
		"@tanstack/react-query": "^5.83.0",
		"normalize.css": "^8.0.1",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-hot-toast": "^2.5.2",
		"react-router-dom": "^7.7.1"
	},
	"devDependencies": {
		"@types/react": "^19.1.9",
		"@types/react-dom": "^19.1.7",
		"@vitejs/plugin-react-swc": "^3.11.0",
		"vite": "^7.0.6",
		"wrangler": "^4.26.1"
	}
}
